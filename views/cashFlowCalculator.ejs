                                    </div>
                                </div>

                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label for="exampleInputEmail1">Tax</label>
                                        <input name="proptax" id="proptax" type="number" class="form-control border-input" placeholder="$200" value="380">
                                    </div>
                                </div>
                                <div class="col-md-4">
                                        <div class="form-group">
                                            <label for="exampleInputEmail1">Management</label>
                                            <input name="mgmt" id="mgmt" type="number" class="form-control border-input" placeholder="$200" value="150">
                                        </div>
                                    </div>
                                
                            </div>
    
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label>Water/Sewer</label>
                                        <input id="water" type="number" class="form-control border-input" placeholder="$100" value="100">
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label>Utilities</label>
                                        <input id="utilities" type="number" class="form-control border-input" placeholder="$150" value="250">
                                    </div>
                                </div>
                            </div>
    
    
                            <div class="row">
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label>Maintenance</label>
                                        <input name="maintenance" id="maintenance" type="number" class="form-control border-input" placeholder="$200" value="250">
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label>Vacancy</label>
                                        <input name="vacancy" id="vacancy" type="number" class="form-control border-input" placeholder="$100" value="200">
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label>Insurance</label>
                                        <input name="insurance" id="ins" type="number" class="form-control border-input" value="150">
                                    </div>
                                </div>
                            </div>





                        
                        <div class="text-center">
                            <button id="btn2" class="btn btn-info btn-fill btn-wd">Evaluate</button>
                            <button id="btn-add" class="btn btn-info btn-fill btn-wd">Add to Database</button>

                        </div>
                        <br><label class="labels1">Monthly Cash Flow: $</label>
                        <input name="cashflow" class="labels1" id="result2">
                        <br><label class="labels1">Cash-on-Cash Return(%):</label>
                        <input name="cashoncash" class="labels1" id="result4">
                        <br><label class="labels1" id="result4">Annual Cash-on-Cash Return: $</label>
                        <input name="annualcashoncash" class="labels1" id="result3">
                        <div class="clearfix"></div>
                        <br><label class="labels1">Cap Rate(%): </label>
                        <input name="caprate" class="labels1" id="result5">
                    </div>
                </div>
            </div>
        </form>
